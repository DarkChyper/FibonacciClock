package constantes;

/**
 * Classe regroupant les constantes de type string
 * 
 * @author darkchyper
 *
 */
public interface ConstantesString {

	// Simple Date Format
	String hh = "hh";
	String mm = "mm";

	String HOUR = "HOUR";
	String MINUTES ="MINUTES";
	
	String ESPACE = " ";
	
	// Liste des palettes de couleur
	String NORMAL = "NORMAL";
	String BLIND_COLOR = "BLIND_COLOR";
	String NO_COLOR = "NO_COLOR";

}
